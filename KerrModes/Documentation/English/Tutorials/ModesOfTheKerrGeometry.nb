 CellID->862586868,ExpressionUUID->"781dc2b7-df09-4b97-a90f-12bf4cc71bb9"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "=", 
    RowBox[{
     SubscriptBox["\[Beta]", "0"], "-", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["\[Alpha]", "0"], 
        SubscriptBox["\[Gamma]", "1"]}], 
       RowBox[{
        SubscriptBox["\[Beta]", "1"], "-"}]], 
      FractionBox[
       RowBox[{
        SubscriptBox["\[Alpha]", "1"], 
        SubscriptBox["\[Gamma]", "2"]}], 
