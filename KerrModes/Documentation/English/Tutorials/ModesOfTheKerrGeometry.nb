 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Beta]", "n"], "\[Congruent]", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      SuperscriptBox["n", "2"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["D", "1"], "+", "2"}], ")"}], "n"}], "+", 
     SubscriptBox["D", "3"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c14f2e22-333c-4e9a-b786-fe7fa19ec50e"],
 ",\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Gamma]", "n"], "\[Congruent]", 
    RowBox[{
     SuperscriptBox["n", "2"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["D", "2"], "-", "3"}], ")"}], "n"}], "+", 
     SubscriptBox["D", "4"], "-", 
     SubscriptBox["D", "2"], "+", "2"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"9f608924-4ae9-4518-a496-773d6d6efc16"],
 "."
}], "DisplayMath",
