Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "n"], "\[Congruent]", 
    RowBox[{
     SuperscriptBox["n", "2"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["D", "0"], "+", "1"}], ")"}], "n"}], "+", 
     SubscriptBox["D", "0"]}]}], TraditionalForm]], "InlineMath",
