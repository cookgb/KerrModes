Cell[CellGroupData[{
Cell["Kerr Total-Transmission Modes Right", "Title",
 CellChangeTimes->{{3.8834078439596233`*^9, 3.88340786764332*^9}, {
  3.883560584347528*^9, 3.883560585099676*^9}},
 CellID->603084473,ExpressionUUID->"455a8fcd-ffcb-4c22-8f3e-4c034eabbc3f"],

Cell[TextData[{
 "Right total-transmission modes of the Kerr geometry can be computed using \
the ",
 ButtonBox["KerrTTMR`",
  BaseStyle->"Link",
  ButtonData->"paclet:KerrTTMR/tutorial/KerrTotal-TransmissionModesRight"],
 " package, building upon the common framework provided by the ",
 ButtonBox["KerrTTMR`",
  BaseStyle->"Link",
  ButtonData->"paclet:KerrTTMR/tutorial/KerrTotal-TransmissionModesRight"],
 " package."
}], "Text",
 CellChangeTimes->{{3.882891785890975*^9, 3.882891806951199*^9}, {
   3.88289183912467*^9, 3.882891926459038*^9}, 3.882892058942791*^9, {
   3.8834079762215633`*^9, 3.883408036012401*^9}, {3.8835606398297377`*^9, 
   3.883560671053553*^9}, {3.88356100194093*^9, 3.8835610035822353`*^9}},
 CellID->900125099,ExpressionUUID->"0017ae0a-f0b4-4f53-ae27-431ba9560c45"],

Cell[CellGroupData[{

Cell["The Teukolsky radial equation", "Section",
 CellChangeTimes->{{3.8828924766430807`*^9, 3.882892500806868*^9}},
 CellID->929480398,ExpressionUUID->"09b722ae-7e06-4a74-9b0d-4d6f43140f11"],

Cell[TextData[{
 "Building on the framework outlined in ",
 ButtonBox["Modes of the Kerr Geometry",
  BaseStyle->"Link",
  ButtonData->"paclet:KerrModes/tutorial/ModesOfTheKerrGeometry"],
 ", ",
 ButtonBox["KerrTTMR`",
  BaseStyle->"Link",
  ButtonData->"paclet:KerrTTMR/tutorial/KerrTotal-TransmissionModesRight"],
 " package make the following specific choices which cause the package to \
find right total-transmission modes:"
}], "Text",
 CellChangeTimes->{{3.882892520937845*^9, 3.882892642313998*^9}, 
   3.882893014566619*^9, {3.883408064325624*^9, 3.8834080834867268`*^9}, 
   3.883560687275285*^9, {3.883561014068626*^9, 3.8835610144949427`*^9}},
 CellID->1300199757,ExpressionUUID->"d63f0a63-b7a0-4d85-9007-611a07ba666b"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Zeta]", "=", 
    RowBox[{
     SubscriptBox["\[Zeta]", "-"], "=", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Omega]"}]}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "eed6398c-2a71-46a3-80b0-39932e9030fe"],
 ", \n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "=", 
    RowBox[{
     SubscriptBox["\[Xi]", "-"], "=", 
     RowBox[{
      RowBox[{"-", "s"}], "-", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["\[Sigma]", "+"]}]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"eda00980-745a-416b-89cb-8a226caa3d08"],
 ", \n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Eta]", "=", 
    RowBox[{
     SubscriptBox["\[Eta]", "+"], "=", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", 
      SubscriptBox["\[Sigma]", "-"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0596de90-3bfa-4785-9b48-ed4ff56e0412"],
 ". "
}], "DisplayMath",
 CellChangeTimes->{{3.882541711999305*^9, 3.882541809400078*^9}, {
